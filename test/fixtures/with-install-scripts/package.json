{
  "name": "with-install-scripts",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "node preinstall.js",
    "install": "node -e \"process.stdout.write('install')\" | json-append output.json",
    "postinstall": "node -e \"process.stdout.write('postinstall')\" | json-append output.json"
  },
  "dependencies": {
    "json-append": "^1.1.1"
  }
}
